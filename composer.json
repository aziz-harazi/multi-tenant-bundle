{
  "name": "aziz-harazi/multi-tenant-bundle",
  "type": "symfony-bundle",
  "description": "Provide 'Multi tenant database' feature for symfony application",
  "keyworkds": [
    "Multi tenant",
    "Database",
    "ORM",
    "DBAL"
  ],
  "license": "MIT",
  "homepage": "https://github.com/aziz-harazi/multi-tenant-bundle.git",
  "authors": [
    {
      "name": "Aziz Harazi",
      "email": "harazi.a@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/config": "^6.0 || ^7.0",
    "symfony/dependency-injection": "^6.0 || ^7.0",
    "symfony/event-dispatcher": "^6.0 || ^7.0",
    "symfony/http-foundation": "^6.0 || ^7.0",
    "symfony/http-kernel": "^6.0 || ^7.0",
    "doctrine/dbal": "^3.0 || ^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-migrations-bundle": "^3.0"

  },
  "autoload": {
    "psr-4": {
      "AzizHarazi\\MultiTenantBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AzizHarazi\\MultiTenantBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "branck-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}